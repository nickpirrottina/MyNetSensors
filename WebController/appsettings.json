{
  "FirstRun": false,
  "Gateway": {
    "SerialGateway": {
      "Enable": true,
      "SerialPortName": "COM3",
      "Boudrate": 115200
    },
    "EthernetGateway": {
      "Enable": false,
      "GatewayIP": "192.168.88.20",
      "GatewayPort": 5003
    },
    "EnableAutoAssignId": true,
    "EnableMessagesLog": true
  },
  "DataBase": {
