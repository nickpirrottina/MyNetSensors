{
  "FirstRun": false,
  "Gateway": {
    "SerialPort": "COM3",
    "EnableAutoAssignId": true,
    "WriteNodesMessagesToDataBase": false,
    "LogState": true,
    "LogMessages": true
  },
  "DataBase": {
    "Enable": true,
    "UseMSSQL": true,
    "MSSQLConnectionString": "Data Source=.\\sqlexpress;Initial Catalog=MyNetSensors;Integrated Security=True",
    "WriteInterval": 5000,
    "LogState": true
  },
  "LogicalNodes": {
    "Enable": true,
    "UpdateInterval": 10,
    "LogEngine": true,
    "LogNodes": true
  },
  "NodesTasks": {
    "Enable": true,
    "UpdateInterval": 10
  },
  "SoftNodes": {
    "Enable": true,
    "Port": 13122,
    "LogState": true,
    "LogMessages": true
  },
  "WebServer": {
    "Enable": true,
    "Debug": false,
    "Logging": {
      "IncludeScopes": false,
      "LogLevel": {
        "Default": "Verbose",
        "System": "Information",
        "Microsoft": "Information"
      }
    }
  }
}