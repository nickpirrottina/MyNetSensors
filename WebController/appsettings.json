{
  "FirstRun": false,
  "SerialGateway": {
    "Enable": true,
    "SerialPort": "COM3",
    "EnableAutoAssignId": true,
    "EnableMessagesLog": true,
    "LogState": true,
    "LogMessages": true
  },
  "DataBase": {
    "Enable": true,
    "UseInternalDb": true,
    "ExternalDbConnectionString": "Data Source=.\\sqlexpress;Initial Catalog=MyNetSensors;Integrated Security=True",
    "WriteInterval": 5000,
    "LogState": true
  },
  "NodesEngine": {
    "Enable": true,
    "UpdateInterval": 10,
    "LogEngine": true,
    "LogNodes": true
  },
  "WebServer": {
    "Enable": true,
    "Debug": false,
    "Logging": {
      "IncludeScopes": false,
      "LogLevel": {
        "Default": "Verbose",
        "System": "Information",
        "Microsoft": "Information"
      }
    }
  }
}